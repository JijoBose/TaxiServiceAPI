<p id="notice"><%= notice %></p>

<h1>Api V1 Rides</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_rides.each do |api_v1_ride| %>
      <tr>
        <td><%= link_to 'Show', api_v1_ride %></td>
        <td><%= link_to 'Edit', edit_api_v1_ride_path(api_v1_ride) %></td>
        <td><%= link_to 'Destroy', api_v1_ride, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Ride', new_api_v1_ride_path %>
